{
  "name": "mxb.cc-server",
  "description": "miaowing's website",
  "private": true,
  "version": "1.0.0",
  "author": "miaowing",
  "repository": "https://github.com/miaowing/mxb.cc",
  "homepage": "https://mxb.cc",
  "license": "MIT",
  "engines": {
    "node": ">=12.16.1"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node src/.keystone",
    "start:dev": "cross-env NODE_ENV=development DISABLE_LOGGING=true nodemon",
    "build": "concurrently -n nest,keystone \"npm run build:nest\" \"npm run build:keystone\"",
    "build:keystone": "cross-env NODE_ENV=production ts-node scripts/build.ts",
    "build:nest": "cross-env NODE_ENV=production tsc --build tsconfig.json",
    "build:public": "cp -r src/public src/.keystone/",
    "build:docker": "docker build -t mxbcc/mxb.cc-server:latest .",
    "postinstall": "ts-node ./scripts/postinstall.ts",
    "clean": "concurrently -n \\\"clean keystone\\\" \\\"rm -rf src/.keystone\\\""
  },
  "nodemonConfig": {
    "delay": 1000
  },
